{
  "version": 1,
  "rules": [
    {
      "enabled": true,
      "description": "Redirect robots.txt to real file",
      "expression": "http.request.uri.path eq \"/robots.txt\"",
      "action": "redirect",
      "status_code": 301,
      "destination": "https://sachavogel.com/files/robots.txt"
    }
  ]
}
